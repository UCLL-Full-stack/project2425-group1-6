datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Vehicle {
  id                Int     @id @default(autoincrement())
  manufacturer      String
  model_name        String
  price             Float
  fuelType         String
  transmissionType String
  year              Int
  vehicleType      String
  bodyType         String
  mileage           Int
  engineCapacity   Int
  createdAt        DateTime
  updatedAt        DateTime

}

model User {
  id                Int     @id @default(autoincrement())
  email             String  @unique
  name              String
  phoneNumber       Int
  password          String
}

